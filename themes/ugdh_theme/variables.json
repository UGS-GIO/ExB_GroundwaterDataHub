      },
      "danger": {
        "100": "#40170a",
        "200": "#6c2711",
        "300": "#983718",
        "400": "#c5461e",
        "500": "#e05f36",
        "600": "#e67f5e",
        "700": "#ec9e86",
        "800": "#f2bead",
        "900": "#f9ded5"
      },
      "light": {
        "100": "#171717",
        "200": "#242424",
        "300": "#313131",
        "400": "#3d3d3d",
        "500": "#4a4a4a",
        "600": "#575757",
        "700": "#646464",
        "800": "#707070",
