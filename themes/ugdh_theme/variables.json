        "500": "#000000",
        "600": "#1a1a1a",
        "700": "#333",
        "800": "#4d4d4d",
        "900": "#666"
      },
      "info": {
        "100": "#2b120a",
        "200": "#512214",
        "300": "#77311d",
        "400": "#9d4126",
        "500": "#c3512f",
        "600": "#d56f50",
        "700": "#df9179",
        "800": "#e9b2a2",
        "900": "#f3d4cb"
      },
      "warning": {
        "100": "#4f3b00",
        "200": "#7d5d00",
        "300": "#aa7f00",
        "400": "#dda400",
        "500": "#ffc300",
        "600": "#ffce2f",
        "700": "#ffde72",
